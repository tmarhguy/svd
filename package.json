{
  "name": "compression-svd",
  "private": true,
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev",
    "dev:web": "pnpm --filter web dev",
    "dev:api": "cd apps/api && poetry run uvicorn main:app --reload --port 8000",
    "dev:all": "concurrently \"pnpm dev:web\" \"pnpm dev:api\"",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "check-types": "turbo run check-types",
    "slides:html": "marp slides/svd.md -o slides/svd.html --allow-local-files",
    "slides:pdf": "marp slides/svd.md -o slides/svd.pdf --pdf --allow-local-files",
    "slides:preview": "marp slides/svd.md --preview --allow-local-files",
    "slides:open": "open slides/svd.html"
  },
  "devDependencies": {
    "@marp-team/marp-cli": "^4.2.2",
    "@svgr/cli": "^8.1.0",
    "concurrently": "^8.2.2",
    "prettier": "^3.6.2",
    "svgo": "^4.0.0",
    "turbo": "^2.5.5",
    "typescript": "5.8.3"
  },
  "packageManager": "pnpm@9.0.0",
  "engines": {
    "node": ">=18"
  }
}
